{
  "layer": "top",
  "position": "top",
  "mod": "dock",
  "exclusive": true,
  "ipc": false,
  "height": 36,
  "passthrough": false,
  "gtk-layer-shell": true,
  "modules-right": ["network", "wireplumber", "backlight", "tray", "clock"],
  "modules-center": ["hyprland/workspaces"],
  "modules-left": ["battery", "disk", "memory", "temperature", "cpu"],
  "hyprland/window": {
    "format": "{}"
  },
  "hyprland/workspaces": {
    "persistent-workspaces": {
      "*": [1, 2, 3, 4, 5], // 7 workspaces by default on every monitor
      "HDMI-A-1": [6, 7, 8, 9, 10] // but only three on HDMI-A-1
    }
  },
  "tray": {
    "show-passive-items": true,
    "icon-size": 18,
    "spacing": 10
  },
  "cpu": {
    "interval": 1,
    "format": "Û∞ª† {icon0}{icon1}{icon2}{icon3}{icon4}{icon5}{icon6}{icon7} {usage}% - {avg_frequency}GHz",
    "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"],
    "tooltip": false
    // "format": "Û∞ª† {usage} {avg_frequency}%"
  },
  "temperature": {
    "thermal-zone": 1,
    "interval": 1,
    "format": "Û∞îè {temperatureC}¬∞C",
    "tooltip": false
  },
  "memory": {
    "interval": 1,
    "format": "Û∞çõ {}%",
    "tooltip": false
  },
  "disk": {
    "interval": 1,
    "format": "Û∞ãä {}%",
    "tooltip": false
  },
  "clock": {
    "format": "{:Û∞•î %I:%M %p}",
    "format-alt": "{:Û∞∏ò %a, %b %e}",
    "tooltip": false
    // "tooltip-format": "<small>{calendar}</small>"
