    //   "format": {
    //     "months": "<span color='#f7768e'><b>{}</b></span>",
    //     "days": "<span color='#7da6ff'><b>{}</b></span>",
    //     "weeks": "<span color='#b9f27c'><b>W{}</b></span>",
    //     "weekdays": "<span color='#e0af68'><b>{}</b></span>",
    //     "today": "<span color='#f7768e'><b><i>{}</i></b></span>"
    //   }
    // }
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{icon} {percent}%",
    "format-icons": ["Û∞Éû", "Û∞Éü", "Û∞É†"],
    "tooltip": false,
    "on-click-right": "xbacklight -dec 5",
    "on-click": "xbacklight -inc 5",
    "min-length": 6
  },
  "wireplumber": {
    "format": "{icon} {volume}%",
    "on-croll-up": "helvum",
    "format-muted": "Û∞ñÅ {volume}%",
    "format-icons": ["Û∞ïø", "Û∞ñÄ", "Û∞ïæ"],
    "tooltip": false,
    "on-click-middle": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggl",
    "on-click-right": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%- && wpctl set-mute @DEFAULT_AUDIO_SINK@ 0",
    "on-click": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+ --limit 1 && wpctl set-mute @DEFAULT_AUDIO_SINK@ 0"
  },
  "battery": {
    "interval": 1,
    "format": "{icon} {capacity}%",
    "format-charging": "Û∞ö• {capacity}%",
    "format-plugged": "Û∞ö• {capacity}%",
    "format-alt": "{time} {icon}",
    "format-icons": ["Û±Éç", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"],
    "tooltip": false
  },
  "network": {
    // "interface": "wlp2*", // (Optional) To force the use of this interface
    "interval": 1,
    // "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"],
    "format-wifi": "{icon} {signalStrength}% - Û∞ÅÜ {bandwidthDownBytes} - Û∞Åû {bandwidthUpBytes}",
    "format-ethernet": "{ipaddr}/{cidr} Û∞àÄ",
    "format-linked": "{ifname} (No IP) Û∞àÄ",
    "format-disconnected": "Disconnected ‚ö†",
    "tooltip": false,

    // "tooltip-format": "{essid} - {ifname} via {gwaddr}"
    "format-icons": ["Û∞§Ø", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"]
    // "format-alt": "{ifname}:{essid} {ipaddr}/{cidr}"
  },
  "mpd": {
    "interval": 1,
    "format": "{stateIcon} {artist} -  {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S})",
    "format-disconnected": "Disconnected ÔÄÅ",
    "format-stopped": "{stateIcon} {artist} -  {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S})",
    "artist-len": 15,
    "title-len": 15,
    "consume-icons": {
      "on": "ÔÉÑ " // Icon shows only when "consume" is on
    },
    "repeat-icons": {
      "on": "ÔÄû "
    },
    "single-icons": {
      "on": "ÔÄû "
    },
    "state-icons": {
      "paused": "Û∞è§",
      "playing": "Û∞êä"
    }
    // "tooltip-format": "MPD (connected)",
    // "tooltip-format-disconnected": "MPD (disconnected)",
    // "on-click": "mpc toggle",
    // "on-click-middle": "alacritty -e ncmpcpp"
  }

